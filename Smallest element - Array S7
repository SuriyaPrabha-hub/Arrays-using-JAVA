import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
         Scanner sc = new  Scanner(System.in);
        int size= sc.nextInt();
        int arr[]= new int[size]; 
        for(int i=0;i<size;i++){
            arr[i]=sc.nextInt();
        }
        Arrays.sort(arr);
        System.out.println(arr[0]);
    }
}

Sample Input 
5
10 20 30 40 50
Sample Output 
10
